{
  "builds": [
    {
      "src": "backend/DjangoApp/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "runtime": "python@3.9"
      }
    },
    {
      "src": "frontend/package.json",
      "use": "@vercel/static-build",
      "config": {
        "buildCommand": "npm run build",
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "source": "/api/(.*)",
      "destination": "backend/DjangoApp/wsgi.py"
    },
    {
      "source": "/(.*)",
      "destination": "/dist/$1"
    }
  ],
  "env": {
    "SECRET_KEY": "YTSXLkZ3276OwSZztgZ3gdJINpR5DXJ8",
    "DEBUG": "False"
  }
}